{
  "name": "golf-shaft-recommendation-ml",
  "version": "1.0.0",
  "description": "Golf shaft recommendation machine learning model using TensorFlow.js - Monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:admin": "concurrently \"npm run dev -w @gears/admin-server\" \"npm run dev -w @gears/admin-client\"",
    "dev:consumer": "concurrently \"npm run dev -w @gears/consumer-server\" \"npm run dev -w @gears/consumer-client\"",
    "build:core": "npm run build -w @gears/core",
    "build:shared-ui": "npm run build -w @gears/shared-ui",
    "build:admin": "npm run build:core && npm run build:shared-ui && npm run build -w @gears/admin-server && npm run build -w @gears/admin-client",
    "build:consumer": "npm run build:core && npm run build:shared-ui && npm run build -w @gears/consumer-server && npm run build -w @gears/consumer-client",
    "build": "npm run build:core && npm run build:shared-ui && npm run build:admin && npm run build:consumer",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "typecheck": "npm run typecheck --workspaces --if-present"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "4.10.0",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
