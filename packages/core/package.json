{
  "name": "@gears/core",
  "version": "1.0.0",
  "description": "Core ML logic for golf shaft recommendations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./inference": {
      "types": "./dist/inference/index.d.ts",
      "default": "./dist/inference/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./training": {
      "types": "./dist/training/index.d.ts",
      "default": "./dist/training/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "4.10.0",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/papaparse": "^5.3.14",
    "typescript": "^5.3.3"
  }
}
